<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import com.noticemedan.cinema.controller.ShowDummy?>
<?import javafx.collections.FXCollections?>

<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import com.noticemedan.cinema.controller.ShowDummy?>
<?import javafx.collections.FXCollections?>

<BorderPane minHeight="720.0" minWidth="1280.0"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.noticemedan.cinema.controller.UIController"
            stylesheets="/css/stylesheet.css"
>
    <top>
        <SplitPane dividerPositions="0.5" orientation="VERTICAL" >
            <items>
                <AnchorPane id="LoginAnchor">
                    <children >
                        <Label text="Enter phone number" layoutX="580.0" layoutY="5.0"/>
                        <Button fx:id="login" onAction="#login" layoutX="700.0" layoutY="35.0" text="Enter" />
                        <TextField fx:id="customerID" layoutX="530.0" layoutY="35.0" promptText="Phone number" />
                        <Label layoutX="853.0" layoutY="35.0" text="Customer:" />
                        <Label fx:id="showID" layoutX="925.0" layoutY="35.0" />
                    </children>
                </AnchorPane>
                <AnchorPane fx:id="PickInfo">
                    <children>
                        <Label text="Pick a date" layoutX="166.0" layoutY="12.0"/>
                        <DatePicker fx:id="pickDate" onAction="#getInfo" layoutX="166.0" layoutY="35.0" prefHeight="30.0" prefWidth="180.0" promptText="Date" />
                        <Label text="Pick a movie" layoutX="411.0" layoutY="12.0"/>
                        <ComboBox fx:id="pickMovie" onAction="#getInfo" layoutX="411.0" layoutY="35.0" prefHeight="30.0" prefWidth="180.0" promptText="Movie" />
                        <Label text="Pick a time" layoutX="665.0" layoutY="12.0"/>
                        <ComboBox fx:id="pickTime" onAction="#getInfo" layoutX="665.0" layoutY="35.0" prefHeight="30.0" prefWidth="180.0" promptText="Time" />
                        <Label fx:id="info" alignment="CENTER" layoutX="935.0" layoutY="25.0" prefHeight="48.0" prefWidth="276.0" >
                            <font>
                                <Font size="15.0" />
                            </font>
                        </Label>
                    </children>
                </AnchorPane>
            </items>
        </SplitPane>
    </top>
    <center>
        <GridPane maxHeight="450.0" maxWidth="884.0" prefHeight="450.0" prefWidth="884.0" BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <CheckBox mnemonicParsing="false" text="CheckBox" />
                <CheckBox mnemonicParsing="false" text="CheckBox" GridPane.rowIndex="1" />
                <CheckBox mnemonicParsing="false" text="CheckBox" GridPane.columnIndex="9" />
                <CheckBox mnemonicParsing="false" text="CheckBox" GridPane.columnIndex="9" GridPane.rowIndex="7" />
                <CheckBox mnemonicParsing="false" text="CheckBox" GridPane.rowIndex="7" />
            </children>
        </GridPane>
    </center>
    <left>
        <SplitPane dividerPositions="0.14" orientation="VERTICAL" prefHeight="200.0" prefWidth="250.0" BorderPane.alignment="CENTER">
            <items>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                    <children>
                        <Button id="NewOrder" onAction="#newOrder" layoutX="65.0" layoutY="8.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="135.0" text="New Order" />
                    </children>
                </AnchorPane>
                <AnchorPane minHeight="200.0" minWidth="0.0" prefHeight="200.0" prefWidth="160.0">
                    <children>
                        <TableView fx:id="tableView" layoutX="3.0" prefHeight="480.0" prefWidth="246.0">
                            <columns>
                                <TableColumn text="Movie">
                                    <cellValueFactory>
                                        <PropertyValueFactory property="movie" />
                                    </cellValueFactory>
                                </TableColumn>
                                <TableColumn text="Time">
                                    <cellValueFactory>
                                        <PropertyValueFactory property="time" />
                                    </cellValueFactory>
                                </TableColumn>
                                <TableColumn text="Date">
                                    <cellValueFactory>
                                        <PropertyValueFactory property="date" />
                                    </cellValueFactory>
                                </TableColumn>
                            </columns>
                        </TableView>
                    </children>
                </AnchorPane>
            </items>
        </SplitPane>
    </left>
    <bottom>
        <Pane prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
                <Button id="DeleteOrder" layoutX="1152.0" layoutY="26.0" mnemonicParsing="false" prefHeight="47.0" prefWidth="102.0" text="Delete" />
                <Button id="SaveOrder" layoutX="1021.0" layoutY="26.0" mnemonicParsing="false" prefHeight="49.0" prefWidth="94.0" text="Save" />
            </children>
        </Pane>
    </bottom>
</BorderPane>
